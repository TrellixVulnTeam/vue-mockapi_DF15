;AAAA,sBACA,MAAA,CAAA,IAAA,KAAA,MADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,KAAA,CACA,GADA,gEACA,EADA,GAEA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA;AADA,iBAFA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,kBAAA,KAAA,CAAA,kBAAA,CAAA,CADA,CAEA;AACA;;AADA;AACA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,iBAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,kBAAA,KAAA,CAAA,SAAA,CAAA;AACA,iBAfA,CAFA;;AAAA;AAAA;AAAA,uBAkBA,KAAA,CAAA,GAAA,gEAAA,EAAA,EAlBA;;AAAA;AAkBA,gBAAA,QAlBA;AAmBA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AApBA;AAAA,sBAsBA,MAAA,CAAA,IAAA,KAAA,OAtBA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAuBA,KAAA,CAAA,GAAA,gEAAA,EAAA,GACA;AACA,kBAAA,WAAA,EAAA,MAAA,CAAA;AADA,iBADA,CAvBA;;AAAA;AA2BA,gBAAA,KAAA,CAAA,kBAAA,CAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,MAAA,CAAA,YAAA,CA5BA,CA6BA;;AA7BA;AA+BA,gBAAA,MAAA,CAAA,YAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,cAAA,GAAA,KAAA;;AAhCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiCA,KA/CA;AAgDA,IAAA,UAhDA,wBAgDA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,KAlDA;AAmDA,IAAA,WAnDA,yBAmDA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,KArDA;AAsDA,IAAA,UAtDA,sBAsDA,EAtDA,EAsDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CACA,MADA,gEACA,EADA,GAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,kBAAA,KAAA,CAAA,uBAAA,CAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,iBANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,kBAAA,KAAA,CAAA,SAAA,CAAA;AACA,iBAVA,CADA;;AAAA;AAYA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AAnEA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"user-detail\">\n    <!-- <h1>{{$route.params.id}}</h1> -->\n    <div class=\"cover\">\n      <Author v-bind:user='user'/>\n      <div class=\"edit-profile\">\n        <button class=\"big-edit\" @click=\"editProfile\">Edit Profile</button>\n        <div class=\"menu-edit\" v-bind:class=\"{showMenuEdit: availableMenuEdit}\">\n          <button @click=\"editName\">Edit Your Name</button>\n          <button @click=\"editPhone\">Edit Phone Number</button>\n          <button @click=\"deleteUser(user.id)\">Delete Account</button>\n          <hr style=\"margin: 1px\">\n          <button @click=\"cancelEdit\">Cancel</button>\n        </div>\n      </div>     \n\n      <div class=\"edit-modal\" v-bind:class=\"{showModal: availableModal}\">\n        <div class=\"modal-content\">\n          <h3>Edit your information below here !</h3>\n          <hr>\n          <form action=\"\"  v-on:submit.prevent=\"successEdit(user.id)\">\n            <input v-model=\"currentInput\" type=\"text\" required>\n            <button type=\"submit\">Submit</button>\n            <button @click=\"cancelModal\">Cancel</button>\n          </form>\n\n        </div>\n      </div>\n    </div>\n    \n    <div class=\"container\">      \n      <p>{{user.information}}</p><br>\n      <p>{{user.information}}</p><br>\n      <p>{{user.information}}</p><br>\n      \n    </div>\n    \n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Author from '../components/uncommon/Author.vue'\n\nexport default {\n  components: {\n    Author\n  },\n  data() {\n    return {\n      user: {},\n      currentInput: null,\n      edit: '',\n      availableModal: false,\n      availableMenuEdit: false\n    }\n    \n  },\n  async created() {\n    const response = await axios.get(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${this.$route.params.id}`)\n    this.user = response.data\n    console.log(response)\n  },\n  methods: {\n    editProfile(){\n      this.availableMenuEdit = true\n    },\n    editName(){\n      this.edit = 'name';\n      this.availableModal = true\n      this.availableMenuEdit = false\n    },\n    editPhone(){\n      this.edit = 'phone';\n      this.availableModal = true\n      this.availableMenuEdit = false\n    },\n    async successEdit(id){\n      if(this.edit === 'name'){\n        await axios\n        .put(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${id}`,\n          {\n            name: this.currentInput,\n          })\n        .then(function (response){\n          alert('Success Updated!')\n          // const response = await axios.get(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${id}`)\n          // location.reload();\n          console.log(response)\n          console.log(response.data)\n        })\n        .catch(function (error){\n          console.log(error)\n          alert('Error !')\n        })\n        const response = await axios.get(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${id}`)\n        console.log(response.data)\n        this.user = response.data\n      }\n      if(this.edit === 'phone'){\n        await axios.put(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${id}`,\n        {\n          phoneNumber: this.currentInput,\n        })\n        alert('Success Updated!')\n        this.user.phoneNumber = this.currentInput\n        //Another way for a faster user experience\n      }\n      this.currentInput = ''\n      this.availableModal = false\n    },\n    cancelEdit(){\n      this.availableMenuEdit = false\n    },\n    cancelModal(){\n      this.availableModal = false\n    },\n    async deleteUser(id){\n      await axios\n      .delete(`https://60d94868eec56d001747768f.mockapi.io/v1/users/${id}`)\n      .then(function(response){\n        alert('You decide to delete!')\n        console.log(response)\n        console.log('Success')\n      })\n      .catch(function (error) {\n        console.log(error)\n        alert('Error !')\n      });\n      this.$router.push(`/users`);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.user-detail{\n  margin: 0;\n  min-height: 30vh;\n}\n.container{\n  width: 55%;\n  text-align: left;\n  margin: 50px auto;\n}\nbutton{\n  margin-right: 15px;\n  background-color: white;\n  padding: 8px 15px;\n  border: 1px solid #f8f8f8;\n  border-radius: 5px;\n  font-size: 0.95rem;\n  transition: 0.4s;\n  cursor: pointer;\n}\nbutton:hover{\n  background-color: #1e2731;\n  color: white;\n}\n.cover{\n  position: relative;\n  padding: 0;\n  height: 50vh;\n  background: linear-gradient(180deg, #f2f2f2, #ffffff);\n  padding-bottom: 50px;\n  box-sizing: border-box;\n  box-shadow: 3px 3px 5px -4px rgba(0,0,0,0.71);\n}\n.author{\n  margin: auto;\n}\n.edit-profile{\n  position: absolute;\n  top: 83%;\n  right: calc(50% - 230px /2);\n  width: 230px;\n  padding: 0;\n}\n  .big-edit{\n    width: 150px;\n    margin: auto;\n    border: 1px solid gray;\n  }\n  .menu-edit{\n    width: 100%;\n    padding: 5px;\n    border: 1px solid gray;\n    border-radius: 5px;\n    background-color: white;\n    box-sizing: border-box;\n    display: none;\n    transition: 0.3s;\n    box-shadow: 3px 4px 10px -5px rgba(0,0,0,0.71);\n  }\n  .showMenuEdit{\n    display: block;\n  }\n    .menu-edit button{\n      width: 100%;\n      text-align: left;\n    }\n\n/* Modal */\n.edit-modal{\n  display: none;\n  position: fixed;\n  z-index: 1;\n  left: 0;\n  top: 0;\n  height: 100%;\n  width: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n.showModal {\n  display: block;\n}\n  .modal-content{\n    margin: 12% auto;\n    padding: 10px;\n    width: 400px;\n    box-shadow: 0 5px 8px 0 rgba(0, 0, 0, 0.2), 0 7px 20px 0 rgba(0, 0, 0, 0.17);\n    background-color: white;\n    border-radius: 6px;\n    animation-name: modalopen;\n  }\n  input{\n    display: block;\n    padding: 15px;\n    border-radius: 10px;\n    background-color: #f3f3f3;\n    color: #2c3e50;\n    width: 90%;\n    margin: 20px auto;\n    border: none;\n    box-sizing: border-box;\n    transition: 0.4s;\n    border: 1px solid white;\n  }\n  input:hover{\n    border: 1px solid gray;\n  }\n  .modal-content button{\n    border: 1px solid #2c3e50;\n  }\n\n@media screen and (max-width: 992px) {\n  .container{\n    width: 75%;\n    text-align: left;\n    margin: 50px auto;\n  }\n}\n\n@keyframes modalopen {\n  from {\n    opacity: 0;\n  }\n  to {\n    opacity: 1;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}